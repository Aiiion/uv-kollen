{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "overrides": [
        {
            "files": ["*.jsx", "*.js"],
            "rules": {
                "react/prop-types": "off",
                "import/extensions": "off",
                "import/no-unresolved": "off",
                "react/jsx-filename-extension": "off"
            }
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    }
}
